<header class="header header-5">
    <div class="header-middle sticky-header">
        <div class="container-fluid">
            <div class="header-left">
                <button class="mobile-menu-toggler">
                    <span class="sr-only">Toggle mobile menu</span>
                    <i class="icon-bars"></i>
                </button>

              

                <nav class="main-nav">
                    <ul class="menu sf-arrows">
                        <a href="/" class="logo">
                            <img src="/assets/images/new/icons8-J.png" alt="Audix Logo" width="70" height="18" style="padding-right: 20px;">
                        </a>
                        <li class="megamenu-container active">
                            <a href="/" style="padding-right: 10px;">Home</a>

                            
                        </li>
                        <li>
                            <a href="/category" class="sf-with-ul">Category &nbsp; &nbsp;</a>
                            
                            <ul style=" padding-right: 0; "> 
                           
                                <% listedCategory.forEach(value => { %>
                                    
                                    <li><a href="/category/<%= value.name %>"><%= value.name%></a></li>
                                    
                             <%  }); %>

                            </ul>

                                

                          
                        </li>
                        <li>
                            <a href="/products" style="padding-right: 18px;">Products</a>

                            
                        </li>
                        <li>
                            <a href="/contact" >Conatct</a>

                            
                        </li>
                        <li>
                            <a href="/About" >About</a>

                            
                        </li>
                     
                    </ul><!-- End .menu -->
                </nav><!-- End .main-nav -->
            </div><!-- End .header-left -->

            <div class="header-right">

                
                <ul>
                    <li>
                        <br>
                       <!-- 
    This is a HTML template with embedded JavaScript logic (using EJS syntax).
    It seems to be part of a web application, specifically a navigation bar or user menu.
-->

<a class="" href="<%= locals.login ? '/profile' : '/login' %>" style="padding-right: 20px;color: #a5620c;">
    <% if (locals.login) { %>
 
        <div class="user-icon-container mt-2">
            
            <img style="width: 40%;" src="assets/images/new/user.png" alt="User" class="user-icon  ">

        </div>
        
    <% } else { %>
        <!-- If the user is not logged in, display "Login" -->
        Login
    <% } %>
</a>

                    </li>
                </ul>
                
               
    
                  


                <div class="header-search header-search-extended header-search-visible">

                    <!-- <a class="text-white " href="">LOGIN</a> -->
                    <a href="#" class="search-toggle" role="button"><i class="icon-search"></i></a>
                    <form action="#" method="get">
                        <div class="header-search-wrapper">
                            <label for="q" class="sr-only">Search</label>
                            <input type="search" class="form-control" name="q" id="q" placeholder="Search product ..." required>
                            <button class="btn btn-primary" type="submit"><i class="icon-search"></i></button>
                        </div><!-- End .header-search-wrapper -->
                    </form>
                </div><!-- End .header-search -->
                
                <a href="/wishlist" class="wishlist-link">
                    <i class="icon-heart-o"></i>
                </a>

                <div class="dropdown cart-dropdown">
                    <a href="/cart" class="dropdown-toggle" role="button"  aria-expanded="false" data-display="static">
                        <i class="icon-shopping-cart"></i>
                        <span class="cart-count">2</span>
                    </a>

                    <!-- End .dropdown-menu -->
                </div><!-- End .cart-dropdown -->
            </div><!-- End .header-right -->
        </div><!-- End .container-fluid -->
    </div><!-- End .header-middle -->
</header><!-- End .header -->



